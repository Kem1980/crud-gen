{
    "name": "kem1980/crud-generator",
    "license": "MIT",
    "description": "Laravel CRUD Generator",
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^6.0|^7.0",
        "laravelcollective/html": "^6.0|^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^5.7|^8.0|^9.0",
        "orchestra/testbench": "^3.3|^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Kem1980\\CrudGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kem1980\\CrudGenerator\\CrudGeneratorServiceProvider"
            ]
        }
    }
}
